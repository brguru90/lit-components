{
  "name": "demo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "npm-run-all --parallel install:angular install:html install:react install:vue",
    "install:angular": "npm install --prefix anguler-demo",
    "install:html": "npm install --prefix html-demo",
    "install:react": "npm install --prefix react-demo",
    "install:vue": "npm install --prefix vue-demo",
    "start": "npm-run-all --parallel start:angular start:html start:react start:vue",
    "start:angular": "npm run start --prefix anguler-demo",
    "start:html": "npm run start --prefix html-demo",
    "start:react": "npm run start --prefix react-demo",
    "start:vue": "npm run dev --prefix vue-demo",
    "clean_install": "npm-run-all --parallel clean_install:angular clean_install:html clean_install:react clean_install:vue",
    "clean_install:angular": "rimraf anguler-demo/node_modules && npm install --prefix anguler-demo",
    "clean_install:html": "rimraf html-demo/node_modules && npm install --prefix html-demo",
    "clean_install:react": "rimraf react-demo/node_modules && npm install --prefix react-demo",
    "clean_install:vue": "rimraf vue-demo/node_modules && npm install --prefix vue-demo"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7"
  }
}
