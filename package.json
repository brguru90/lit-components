{
  "name": "vg",
  "private": true,
  "version": "0.0.53",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-ts": "tsc --build tsconfig-module.json",
    "build-js": "vite build --config vite-module.config.ts --mode development",
    "build-module": "npm-run-all --serial build-ts build-js",
    "preview": "vite preview",
    "lsp-support": "custom-elements-manifest analyze --config custom-elements-manifest.config.mjs",
    "pack-old": "touch vg.tgz && tar -czf vg.tgz  --exclude='vg.tgz' --exclude='node_modules' -C ./dist/ .",
    "pack": "npm pack",
    "update_version": "npm version patch --no-git-tag-version",
    "fix_stuff": "node scripts/fix.js",
    "update-demos": "node scripts/update-demos.js",
    "install-demos": "npm install --prefix demo",
    "release_test": "npm-run-all --serial build-module lsp-support",
    "release": "npm-run-all --serial build-module lsp-support fix_stuff update_version pack update-demos install-demos",
    "demo": "npm start --prefix demo",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:watch": "test-storybook --watch",
    "lighthouse": "npm run build-storybook && lhci autorun",
    "lighthouse:server": "lhci server"
  },
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./index.css": "./dist/index.css",
    "./react": "./dist/react/index.js",
    "./vue": "./dist/vue/index.js",
    "./jsx": "./dist/jsx/index.js"
  },
  "dependencies": {
    "@storybook/react": "^9.1.10",
    "@storybook/vue3": "^9.1.10",
    "@storybook/vue3-vite": "^9.1.10",
    "@vue/compiler-sfc": "^3.5.22",
    "@vueuse/core": "^13.9.0",
    "@wc-toolkit/jsdoc-tags": "^1.1.0",
    "lit": "^3.1.2",
    "vue": "^3.5.22",
    "vue-tsc": "^3.1.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1800.0",
    "@angular-devkit/build-angular": "^18.0.0",
    "@angular-devkit/core": "^18.0.0",
    "@angular/animations": "^18.0.0",
    "@angular/cli": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@chromatic-com/storybook": "^4.1.1",
    "@custom-elements-manifest/analyzer": "^0.10.8",
    "@lhci/cli": "^0.15.1",
    "@lhci/server": "^0.15.1",
    "@storybook/addon-backgrounds": "^9.0.8",
    "@storybook/addon-docs": "^9.1.8",
    "@storybook/angular": "^9.1.0",
    "@storybook/test-runner": "^0.23.0",
    "@storybook/web-components-vite": "^9.1.8",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@wc-toolkit/jsx-types": "^1.4.2",
    "@wc-toolkit/storybook-helpers": "^9.0.1",
    "@web/dev-server-legacy": "^2.1.1",
    "cem-plugin-expanded-types": "^1.4.0",
    "cem-plugin-reactify": "^0.0.2",
    "cem-plugin-vs-code-custom-data-generator": "^1.4.2",
    "chrome-launcher": "^1.2.1",
    "cors": "^2.8.5",
    "custom-element-react-wrappers": "^1.7.2",
    "custom-element-vs-code-integration": "^1.5.0",
    "custom-element-vuejs-integration": "^1.4.0",
    "i": "^0.3.7",
    "lighthouse": "^12.8.2",
    "npm": "^10.8.1",
    "npm-run-all": "^4.1.5",
    "rxjs": "^7.8.1",
    "sass": "^1.77.3",
    "sqlite3": "^5.1.7",
    "storybook": "^9.1.8",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-cem": "^0.8.0",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-tsconfig-paths": "^4.3.2",
    "zone.js": "^0.14.0"
  },
  "customElements": "dist/custom-elements.json"
}
